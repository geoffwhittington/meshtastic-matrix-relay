[run]
source = src/mmrelay
omit =
    tests/**/*
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */build/*
    */dist/*
    */setup.py
    */conftest.py
branch = True
relative_files = False

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
ignore_errors = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
